                                                                                    if(strcmp(")",firstT->arrT)==0) next();
                                                                                        else{printf("line is%d i read %s corect is)\n",firstT->lineT,firstT->arrT);
                                                                                             next();}
                                                                                              if(token()==';') {next();                                                           }
                                                                                                 else{printf("line is%d i read %s corect is;\n",firstT->lineT,firstT->arrT);
                                                                                                 next(); return 0; }}

